{
	"name": "dynamic-meter",
	"description": "A customizable meter/gauge panel for Directus that displays numeric values with dynamic thresholds and collection-based or manual maximum values",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-panel"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "panel",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"dependencies": {
		"@directus/extensions-sdk": "^10.1.14",
		"vue": "^3.3.4",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^10.1.14",
		"@vitejs/plugin-vue": "^4.2.3",
		"typescript": "^5.0.0",
		"vite": "^4.3.9"
	}
}
